{
  "name": "theme-color",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start -p 8080",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "test": "jest src/__tests__/.*/*.test.tsx?$",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "apollo-codegen": "apollo client:codegen --target typescript --passthroughCustomScalars --globalTypesFile=src/graphql/globalTypes/globalTypes.ts",
    "puppeteer-storyshots": "jest src/__tests__/puppeteer-storyshots*.runner.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test && npm run puppeteer-storyshots"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-storyshots": "^6.1.21",
    "@storybook/addon-storyshots-puppeteer": "^6.1.21",
    "@testing-library/react": "^11.2.6",
    "@types/chroma-js": "^2.1.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "babel-preset-react-app": "^10.0.0",
    "chroma-js": "^2.1.1",
    "cross-fetch": "^3.1.4",
    "date-fns": "^2.21.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "graphql": "^15.5.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-css-modules": "^2.1.0",
    "jest-dom": "^4.0.0",
    "next": "^10.2.3",
    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "puppeteer": "^9.1.0",
    "react": "17.0.2",
    "react-circle-slider": "^1.6.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "^17.0.2",
    "recoil": "^0.2.0",
    "styled-components": "^5.2.3",
    "styled-media-query": "^2.1.2",
    "tailwindcss": "^2.1.2",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^3.4.12",
    "@types/styled-components": "^5.1.9",
    "babel-loader": "^8.2.2",
    "eslint-plugin-react-hooks": "^4.2.0"
  }
